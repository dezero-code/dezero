{
  "name": "dezero/dezero",
  "description": "Dezero Framework",
  "type": "project",
  "keywords": ["yii2", "dezero", "Framework", "dz2"],
  "homepage": "https://github.com/dezero-code/dezero",
  "authors": [
    {
      "name": "Dezero",
      "homepage": "http://www.dezero.es",
      "email": "code@dezero.es"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.4",
    "kartik-v/yii2-widget-fileinput": "dev-master",
    "kint-php/kint": "~3.3",
    "lsat/yii2-bower-asset": "~2.0.13",
    "mikehaertl/php-shellcommand": "^1.6.0",
    "mikehaertl/phpwkhtmltopdf": "^2.5.0",
    "phpoffice/phpspreadsheet": "^1.27.0",
    "spatie/image": "^2.0.0",
    "vlucas/phpdotenv": "^5.4.0",
    "voku/stringy": "^6.4.0",
    "yiisoft/yii2": "~2.0.5",
    "yiisoft/yii2-bootstrap4": "~2.0.6",
    "yiisoft/yii2-bootstrap5": "~2.0.4",
    "yiisoft/yii2-httpclient": "*",
    "yiisoft/yii2-queue": "~2.3.2",
    "yiisoft/yii2-redis": "~2.0.0",
    "yiisoft/yii2-symfonymailer": "~3.0.0"
  },
  "require-dev": {
    "yiisoft/yii2-gii": "~2.2.0"
  },
  "autoload": {
    "psr-4": {
      "dezero\\": "src/"
    },
    "exclude-from-classmap": [
      "src/Dz.php",
      "src/"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "yiisoft/yii2-composer" : true
    },
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "scripts": {
    "coder": "php-cs-fixer fix -vvv --dry-run --show-progress=dots",
    "coder-fix": "php-cs-fixer fix -vvv --show-progress=dots"
  }
},
